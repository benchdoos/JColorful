{
  "type": "jColorfulTheme",
  "name": "Extremely Black",
  "version": 1,
  "author": "Eugene Zrazhevsky",
  "theme": [
    {
      "objectType": "common",
      "background": "#000000",
      "foreground": "#eeeeee"
    },
    {
